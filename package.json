{
  "name": "vue-intercom",
  "version": "1.0.4",
  "description": "An Intercom plugin for Vue.js",
  "contributors": [
    "Tyrone Tudehope <tyronetudehope@gmail.com>"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "continuon/vue-intercom",
  "scripts": {
    "bundle:web": "rm -rf dist && webpack --config build/webpack.bundle.conf.js",
    "bundle:lib": "rm -rf lib && babel src --out-dir lib",
    "build": "npm run bundle:lib && npm run bundle:web",
    "unit": "karma start test/unit/karma.conf.js --watch",
    "test": "karma start test/unit/karma.conf.js --single-run",
    "lint": "eslint --ext .js src test/unit/index.js test/unit/specs",
    "prepublish": "npm run lint && npm test && npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.7.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^0.9.0",
    "function-bind": "^1.0.2",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.3",
    "sinon": "^1.17.3",
    "vue": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-merge": "^2.4.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
